{
  "name": "phash-js",
  "version": "0.0.1",
  "description": "Perceptual image hashing for browsers",
  "main": "dist/phash.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "browserify src/index.js -o dist/phash.js",
    "test": "npm run build && npx jest"
  },
  "keywords": [
    "phash",
    "perceptual",
    "hash",
    "wasm",
    "webassembly",
    "imagemagick"
  ],
  "author": "Arhey",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "wasm-imagemagick": "^1.2.8"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/plugin-transform-runtime"
          ]
        }
      ]
    ]
  }
}
